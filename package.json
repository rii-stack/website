{
	"name": "rii-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm run version && vite dev",
		"build": "npm run version && vite build",
		"preview": "npm run version && vite preview",
		"check": "npm run version && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "npm run version && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"version": "check-node-version --node $(cat .nvmrc)"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.79.4",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"check-node-version": "^4.2.1"
	}
}
